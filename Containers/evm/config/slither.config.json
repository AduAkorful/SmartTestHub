{
  "detectors_to_exclude": [],
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "solc_disable_warnings": false,
  "json": "/app/logs/slither/slither-report.json",
  "solc_remaps": ["@openzeppelin/=node_modules/@openzeppelin/", "@chainlink/=node_modules/@chainlink/"],
  "exclude": ["test/", "scripts/", "migrations/"],
  "exclude-dirs": ["node_modules", "cache", "artifacts", "out", "lib"],
  "paths": ["contracts/"],
  "fail-fast": false,
  "filter-paths": true,
  "exclude-dependencies": true,
  "exclude-optimization": false,
  "checklist": true,
  "markdown-root": "logs/",
  "disable-color": true,
  "detectors-to-run": [
    "suicidal", "uninitialized-state", "uninitialized-storage", "arbitrary-send",
    "controlled-delegatecall", "reentrancy-eth", "reentrancy-no-eth", 
    "reentrancy-unlimited-gas", "tx-origin", "assembly", "assert-state-change",
    "boolean-equal", "dangerous-unary", "deprecated-standards", "divide-before-multiply",
    "enum-conversion", "erc20-interface", "erc721-interface", "incorrect-equality",
    "locked-ether", "low-level-calls", "missing-zero-check", "pragma", 
    "redundant-statements", "solc-version", "unchecked-lowlevel", "unchecked-send",
    "uninitialized-local", "unused-return", "unused-state", "costly-loop", 
    "dead-code", "reentrancy-benign", "reentrancy-events", "timestamp", "too-many-digits"
  ]
}
